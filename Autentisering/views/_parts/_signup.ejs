<div class="backgroundImage"></div>
<div class="container">
    <fieldset id="registrationForm" class="registration form">
        <h2 class="header">Bli Medlem</h2>
        <form method="POST" action="/register">
            <label for="name">Ange ditt Namn:</label>
            <input type="text" id="name" name="username" placeholder="John Depp" autocomplete="name">
            <!-- <label for="adress">Ange din adress:</label>
        <input type="text" id="adress" placeholder="Dröttningsgatan 5" autocomplete="address-level1">
        <label for="tel">Ange din tel:</label>
        <input type="tel" id="tel" placeholder="076 233 456 3" autocomplete="tel"> -->
            <label for="e-mail">Ange din e-mail:</label>
            <input type="text" id="e-mail" name="email" placeholder="john.depp@example.com" autocomplete="username">
            <label for="passwords">Skapa ett lösenord:</label>
            <input type="password" name="password" id="passwords" placeholder="Skapa ett lösenord"
                autocomplete="new-password">
            <label for="password2">Bekräfta ditt lösenord:</label>
            <input type="password" name="password2" id="password2" placeholder="Bekräfta ditt lösenord"
                autocomplete="new-password">
            <label style="display: none;" for="Medlem">Bli Medlem</label>
            <input type="submit" id="Medlem" class="button" value="Bli Medlem">
        </form>
